{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "globals": {
    "__DEV__": true,
    "Promise": true
  },
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "radix": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "linebreak-style": "off",
    "no-use-before-define": 0,
    "camelcase":"off",
    "prefer-template": "off",
    "class-methods-use-this": "off",
    "react/forbid-prop-types": [0],
    "react/require-default-props": 0,
    "react/prefer-stateless-function":0,
    "no-unused-vars":"warn",
    "no-restricted-syntax":0
  }
}
